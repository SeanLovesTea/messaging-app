<div class="friendList">
  <h2>Friend List</h2>
  <% userData.forEach(user => { %>
    <a href="/messages/<%= user._id %>">
      <div class="chats" onclick={getReceiver}>
        <div class="chats__avatar"><img src="./images/default.png" alt=""></div>
        <div class="chats__box">
          <div class="chats__name"><%= user.username %></div>
          <div class="chats__message">Hey, you are the best</div>
        </div>
      </div>
    </a>
  <% }) %>
  <div class="chats">
    <div class="chats__avatar"><img src="./images/default.png" alt=""></div>
    <div class="chats__box">
      <div class="chats__name">Louiseypoo</div>
      <div class="chats__message">Hey, you are the best</div>
    </div>
  </div>
  <div class="chats">
    <div class="chats__avatar"><img src="./images/default.png" alt=""></div>
    <div class="chats__box">
      <div class="chats__name">Jimbo</div>
      <div class="chats__message">Hey, you are the worst</div>
    </div>
  </div>
</div>
